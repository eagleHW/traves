<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape>
            <gradient
                android:startColor="#ff338219"
                android:endColor="#ff338219"
                android:angle="270" />

            <corners
                android:radius="2dp" />
            <padding
                android:left="25dp"
                android:top="15dp"
                android:right="25dp"
                android:bottom="15dp" />
        </shape>
    </item>

    <item>
        <shape>

            <gradient
                android:startColor="#ff34b516"
                android:endColor="#ff338219"
                android:angle="270" />


            <corners
                android:radius="2dp" />
            <padding
                android:left="25dp"
                android:top="15dp"
                android:right="25dp"
                android:bottom="15dp" />
        </shape>
    </item>
</selector>